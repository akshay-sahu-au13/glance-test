{
  "name": "webpeek-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for glance webpeeks",
  "main": "index.ts",
  "scripts": {
    "dev": "parcel index.html",
    "build": "rm -rf dist/ && parcel build index.html --no-source-maps --public-url '.'",
    "lint": "eslint -c .eslintrc --ext .tsx,.ts src/",
    "lint:fix": "eslint --fix -c .eslintrc --ext .tsx,.ts src/",
    "test": "jest --coverage --coverageDirectory=target/",
    "test:dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.tools.inmobi.com:glance/webpeek-boilerplate.git"
  },
  "jest-junit": {
    "suiteName": "Jest JUnit Unit Tests",
    "outputName": "./junit.xml",
    "outputDirectory": "target"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.3.1",
    "goober": "^2.0.30",
    "preact": "^10.5.12",
    "preact-router": "^3.2.1",
    "react-lottie": "^1.2.3",
    "src": "^1.1.2"
  }
}
